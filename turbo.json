{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "lint": {},
    "lint:fix": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "ba:generate": {
      "cache": false
    }
  }
}
